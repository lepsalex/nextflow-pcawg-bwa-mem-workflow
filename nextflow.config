process.container = 'quay.io/pancancer/pcawg-bwa-mem:latest'

docker {
    enabled = true
}

timeline {
    enabled = true
    file = 'reports/timeline.html'
}

report {
    enabled = true
    file = 'reports/report.html'
}

dag {
    enabled = true
    file = 'reports/flowchart.html'
}

k8s {
    namespace = 'nextflow-pcawg'
}